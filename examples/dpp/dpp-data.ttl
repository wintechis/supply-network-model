dppdata:dpp a dpp-odp:DPP ;
        dpp-odp:describes dppdata:product ;
        dpp-info:containsInformation dppdata:cf .
dppdata:product a dpp-odp:Product ;
        dpp-odp:hasPart dppdata:component .
dppdata:cf a dpp-info:DPPInformation .
_:derivation a psn:Derivation ;
        psn:from dppdata:component ;
        psn:into dppdata:product ;
        psn:quantity 4.0 .
_:composition a ceon-product:Composition ;
        ceon-product:compositionOf dppdata:product ;
        ceon-product:compositionQuantity 4.0 .
_:bom a pv:BillOfMaterial ;
        pv:hasBomPart _:bompart .
_:bompart a pv:BomPart ;
        pv:hasPart dppdata:component
        pv:quantity "4" .