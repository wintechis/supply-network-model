# own prefix
@prefix sn: <https://purl.org/sn/vocab#> .
@prefix csn: <https://example.org/csn/vocab#> .
@prefix osn: <https://example.org/osn/vocab#> .
@prefix isn: <https://example.org/isn/vocab#> .

# other prefixes
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


# --- classes ---

csn:Country a owl:Class ;
    rdfs:subClassOf sn:Entity  ;
    rdfs:label "Country"@en ;
    rdfs:comment "todo"@en ;
    sn:expresses csn:CountryAbstraction .

csn:Trade a owl:Class ;
    rdfs:subClassOf sn:Relation  ;
    rdfs:label "Trade"@en ;
    rdfs:comment "todo"@en ;
    sn:expresses csn:CountryAbstraction .


# --- object properties ---
    
csn:inContinent a owl:ObjectProperty ;
    rdfs:label "in continent"@en ;
    rdfs:comment "todo"@en ;
    rdfs:subPropertyOf sn:associatedEntity ;
    rdfs:domain csn:Country ;
    rdfs:range osn:Continent .


# --- individuals ---

csn:CountryAbstraction a sn:Abstraction ;
    rdfs:label "Country abstraction"@en ;
    rdfs:comment "todo"@en ;
    sn:higherAbstractionLevel osn:ContinentAbstraction ;
    sn:similarAbstractionLevel isn:IndustryAbstraction .
